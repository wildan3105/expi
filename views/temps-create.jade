html
  head
    title=title
  body
    p Create temp
    form(name='temp', method='post', action='./')
      label Value : 
      input(type='number', name='name')
      select(name='category')
        option(value='low') LOW 
        option(value='middle') MIDDLE 
        option(value='high') HIGH
      select(name='safe')
        option(value='yes') YES
        option(value='no') NO
      input(type='submit')
    
    a(href='./') Back to temps
